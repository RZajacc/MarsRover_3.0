{
  "version": "1.0.0",
  "description": "JS Project connecting with Mars Rover API from Nasa",
  "author": "Rafal Zajac",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack",
    "build:dev": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack-dev-server",
    "build:prod": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack --config webpack.config.prod.js",
    "test": "vitest --run --reporter verbose --environment jsdom",
    "coverage": "vitest run --coverage",
    "ui": "vitest --ui",
    "cypress:open": "cypress open",
    "sass:prod": "sass --no-source-map --style=compressed src/styles/scss:src/styles/css",
    "sass:dev": "sass --watch --no-source-map --update --style=expanded src/styles/scss:src/styles/css"
  },
  "dependencies": {
    "@emailjs/browser": "^3.11.0",
    "cross-env": "^7.0.3",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@vitest/coverage-v8": "^1.2.2",
    "@vitest/ui": "^1.2.2",
    "babel-loader": "^9.1.3",
    "css-loader": "^7.1.0",
    "cypress": "^13.7.0",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "happy-dom": "^13.3.8",
    "html-webpack-plugin": "^5.6.0",
    "prettier": "3.2.4",
    "sass": "^1.74.1",
    "sass-loader": "^14.1.1",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
