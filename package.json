{
  "version": "1.0.0",
  "description": "JS Project connecting with Mars Rover API from Nasa",
  "author": "Rafal Zajac",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack",
    "test": "vitest --run --reporter verbose --environment jsdom",
    "coverage": "vitest run --coverage",
    "ui": "vitest --ui",
    "cypress:open": "cypress open",
    "sass:prod": "sass --no-source-map --style=compressed src/styles/scss:src/styles/css",
    "sass:dev": "sass --watch --no-source-map --update --style=expanded src/styles/scss:src/styles/css"
  },
  "dependencies": {
    "@emailjs/browser": "^3.11.0",
    "cross-env": "^7.0.3",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@vitest/coverage-v8": "^1.2.2",
    "@vitest/ui": "^1.2.2",
    "cypress": "^13.7.0",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "happy-dom": "^13.3.8",
    "prettier": "3.2.4",
    "sass": "^1.74.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4"
  }
}
